{
  "name": "service-booking-api",
  "version": "1.0.0",
  "description": "Service booking management backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migrate:create": "sequelize-cli migration:generate --name",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:create": "sequelize-cli seed:generate --name",
    "seed:dev": "NODE_ENV=development npm run seed",
    "seed:test": "NODE_ENV=test npm run seed",
    "seed:fresh": "npm run seed:undo && npm run seed",
    "db:setup": "npm run migrate && npm run seed",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed",
    "seed:validate": "ts-node src/database/validate-seeds.ts",
    "seed:verify": "ts-node src/database/verify-seeds.ts",
    "db:verify": "ts-node src/database/setup-verification.ts",
    "db:init": "ts-node src/database/init.ts",
    "db:stats": "ts-node -e \"import('./src/database/database-utils').then(m => m.DatabaseUtils.getDatabaseStats().then(console.log))\"",
    "db:health": "ts-node -e \"import('./src/database/init').then(m => m.databaseHealthCheck().then(console.log))\"",
    "serve": "ts-node src/server.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "k6:smoke": "k6 run k6/scenarios/smoke-test.js",
    "k6:load": "k6 run k6/scenarios/load-test.js",
    "k6:stress": "k6 run k6/scenarios/stress-test.js",
    "admin:seed": "sequelize-cli db:seed --seed 20240917-admin-user.js"
  },
  "keywords": [
    "api",
    "service",
    "booking",
    "typescript",
    "koa"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@koa/router": "^14.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/context-async-hooks": "^2.1.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.5.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-cookies": "^5.0.0",
    "koa-cors": "^0.0.16",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "koa-swagger-ui": "^1.1.3",
    "koa-views": "^8.1.0",
    "koa2-swagger-ui": "^5.12.0",
    "node-schedule": "^2.1.1",
    "sequelize": "^6.35.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.29.0",
    "systeminformation": "^5.27.10",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/koa": "^2.13.12",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-cors": "^0.0.5",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/koa-views": "^2.0.4",
    "@types/node": "^20.10.5",
    "@types/node-schedule": "^2.1.8",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
